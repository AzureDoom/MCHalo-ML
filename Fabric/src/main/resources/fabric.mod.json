{
    "schemaVersion": 1,
    "id": "mchalo",
    "version": "${version}",
    "name": "Rename",
    "description": "Adds weapons from the Halo franchise to Minecraft!",
    "authors": [
      "AzureDoom"
    ],
    "contact": {
      "homepage": "https://fabricmc.net/",
      "sources": "https://github.com/AzureDoom/Azure-MultiLoader-Template"
    },
    "license": "MIT",
    "icon": "icon.png",
    "environment": "*",
    "entrypoints": {
      "main": [
        "mod.azure.mchalo.MCHaloMod"
      ],
      "client" : [
        "mod.azure.mchalo.client.MCHaloModFabricClient"
      ]
    },
    "mixins": [
      "mchalo.mixins.json",
      "mchalo.fabric.mixins.json"
    ],
    "depends": {
      "fabricloader": ">=0.14",
      "fabric": "*",
      "minecraft": "1.19.3",
      "azurelib": ">=1.0.2",
      "java": ">=17"
    },
    "suggests": {
      "another-mod": "*"
    },
    "accessWidener": "mchalo_common.accesswidener"
  }
  
