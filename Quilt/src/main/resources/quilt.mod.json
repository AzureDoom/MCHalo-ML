{
  "schema_version": 1,
  "quilt_loader": {
    "group": "mod.azure.mchalo",
    "id": "mchalo",
    "version": "${version}",
    "metadata": {
      "name": "MCHalo",
      "description": "Adds weapons from the Halo franchise to Minecraft!",
      "contributors": {
        "AzureDoom": "Author"
      },
      "contact": {
        "sources": "https://github.com/AzureDoom/MCHalo-ML"
      },
      "icon": "icon.png"
    },
    "intermediate_mappings": "net.fabricmc:intermediary",
    "entrypoints": {
      "init": [
        "mod.azure.mchalo.MCHaloMod"
      ],
      "client_init": [
        "mod.azure.mchalo.client.MCHaloModQuiltClient"
      ]
    },
    "depends": [
      {
        "id": "quilt_loader",
        "versions": ">=0.18.0-"
      },
      {
        "id": "minecraft",
        "versions": ">=1.19.3"
      },
      {
        "id": "azurelib",
        "versions": ">=1.0.2"
      },
      {
        "id": "quilt_base",
        "versions": ">=4.0.0-"
      }
    ]
  }
  }
  
